<Window x:Class="Check_IT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Check_IT" Height="200" Width="320" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="200">
            <Button x:Name="OpenRegisterBtn" Click="OpenRegisterBtn_Click" Content="Register" Height="36"/>
            <Button x:Name="OpenLoginBtn" Click="OpenLoginBtn_Click" Content="Login" Height="36"/>
        </StackPanel>
    </Grid>
</Window>
